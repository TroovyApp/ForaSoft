    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "moment": "^2.19.1",
    "normalizr": "^3.2.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-dotdotdot": "^1.1.0",
    "react-ellipsis-text": "^1.0.0",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-toastify": "^2.1.6",
    "react-truncate": "^2.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.0",
    "style-loader": "0.18.2",
