<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>$(troovy_display_name)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.3</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.troovy.forasoft.application</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>troovy</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>18</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<string>NO</string>
		<key>NSExceptionDomains</key>
		<dict/>
	</dict>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Used to connect via bluetooth.</string>
	<key>NSCameraUsageDescription</key>
	<string>This way you will be able to make photos for your workshop.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This way you will be able to get information about nearby workshops.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>This way you will be able to stream and receive audio.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>This way you will be able to choose image from you photos.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This way you will be able to choose image from you photos.</string>
	<key>TroovyConstantsDictionary</key>
	<dict>
		<key>apple_merchant_identifier</key>
		<string>$(troovy_apple_merchant_identifier)</string>
		<key>branch_key</key>
		<string>$(troovy_branch_key)</string>
		<key>google_analytics_filename</key>
		<string>$(troovy_google_analytics_filename)</string>
		<key>hockeyapp_app_identifier</key>
		<string>$(troovy_hockeyapp_app_identifier)</string>
		<key>in_app_product_template</key>
		<string>$(troovy_in_app_template)</string>
		<key>server_url</key>
		<string>$(troovy_server_url)</string>
		<key>socket_server_url</key>
		<string>$(troovy_socket_server_url)</string>
		<key>stripe_api_key</key>
		<string>$(troovy_stripe_api_key)</string>
		<key>stun_server_url</key>
		<string>$(troovy_stun_server_url)</string>
		<key>turn_server_credential</key>
		<string>$(troovy_turn_server_credential)</string>
		<key>turn_server_url</key>
		<string>$(troovy_turn_server_url)</string>
		<key>turn_server_username</key>
		<string>$(troovy_turn_server_username)</string>
	</dict>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UIRequiresFullScreen</key>
	<true/>
	<key>UIStatusBarHidden</key>
	<true/>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleLightContent</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>
</dict>
</plist>
